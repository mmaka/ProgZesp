        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rozmieszczenie.Widoki"
        mc:Ignorable="d"
        Title="Utwórz PDF" Height="300" Width="312.245">
    <Grid>
        <DockPanel>
            <Label DockPanel.Dock="Top">Wybierz inforamacje które mają zostać dodane do pliku</Label>
            <Button DockPanel.Dock="Bottom" Click="Button_Click" > OK</Button>
                <ListBox x:Name="listBox"  SelectionMode="Multiple" Margin="0,28.4,0,0.4">

                <ListBoxItem Content="Informacje Dodatkowowe"></ListBoxItem>
